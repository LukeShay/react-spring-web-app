{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "module": "es6",
    "jsx": "react",
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "plugins": [{ "name": "typescript-tslint-plugin" }]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
