{
    "name": "front_end",
    "version": "0.2.4",
    "private": true,
    "homepage": "http://lukeshay.com",
    "dependencies": {
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.2.0",
        "react-toastify": "^5.4.0"
    },
    "scripts": {
        "prestart": "yarn",
        "start": "webpack-dev-server --open --config webpack.config.js --env.env=dev",
        "prebuild": "rm -rf dist && yarn",
        "build": "webpack --config webpack.config.js --env.env=prod",
        "build:analyze": "npm run build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
        "deploy": "pm2 serve dist --name web-app -- 3000"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@babel/preset-env": "^7.7.1",
        "@babel/preset-react": "^7.7.0",
        "@babel/runtime": "^7.7.2",
        "babel-loader": "^8.0.6",
        "bootstrap": "^4.3.1",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^6.0.3",
        "css-loader": "^3.2.0",
        "flux": "^3.1.3",
        "html-webpack-plugin": "^3.2.0",
        "install": "^0.13.0",
        "json-server": "^0.15.1",
        "npm-run-all": "^4.1.5",
        "style-loader": "^1.0.0",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.41.2",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0",
        "webpack-merge": "^4.2.2",
        "webpack-visualizer-plugin": "^0.1.11"
    },
    "main": "src/index.js",
    "author": "Luke Shay",
    "license": "MIT"
}
