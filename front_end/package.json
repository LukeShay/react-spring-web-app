{
  "name": "front_end",
  "version": "0.4.1",
  "private": true,
  "homepage": "http://lukeshay.com",
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-toastify": "^5.4.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2"
  },
  "scripts": {
    "prestart": "yarn",
    "start": "webpack-dev-server --open --config webpack.config.js --env.env=dev",
    "prebuild": "yarn",
    "build": "webpack --config webpack.config.js --env.env=prod",
    "build:analyze": "npm run build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "deploy": "pm2 serve dist --name web-app -- 3000",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint:fix",
      "pre-push": "yarn format && yarn lint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "@loadable/component": "^5.11.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "flux": "^3.1.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "install": "^0.13.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "style-loader": "^1.0.0",
    "universal-cookie": "^4.0.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "main": "src/index.js",
  "author": "Luke Shay",
  "license": "MIT"
}
