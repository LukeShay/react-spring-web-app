    "@babel/preset-react": "^7.7.4",
    "@loadable/component": "^5.11.0",
    "@types/flux": "^3.1.9",
    "@types/jest": "^24.0.25",
    "@types/universal-cookie": "^2.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.4.1",
    "clean-webpack-plugin": "*",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "flux": "^3.1.3",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "node": "^13.3.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
