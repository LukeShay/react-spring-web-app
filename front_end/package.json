{
  "name": "front_end",
  "version": "0.4.3",
  "private": true,
  "homepage": "http://lukeshay.com",
  "main": "src/index.tsx",
  "author": "Luke Shay",
  "license": "LGPL",
  "scripts": {
    "start": "webpack-dev-server --open --hot --config configs/webpack/dev.js",
    "prebuild": "yarn",
    "build": "webpack -p --config=configs/webpack/prod.js",
    "build:analyze": "yarn build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "deploy": "pm2 serve dist --name web-app -- 3000",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "format": "prettier --write \"**/*.+(ts|tsx|json|css|md)\""
  },
  "dependencies": {
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@types/node": "^13.1.1",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-toastify": "^5.4.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
